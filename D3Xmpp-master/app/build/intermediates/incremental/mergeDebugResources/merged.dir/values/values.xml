    <style name="ContactCate">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff424242</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">#00000000</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ContactListItem">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ContactName">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">#424242</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
    </style>
    <style mce_bogus="1" name="PopupAnimation" parent="android:Animation">
		<item name="android:windowEnterAnimation">@anim/pops</item>
		<item name="android:windowExitAnimation">@anim/pope</item>
